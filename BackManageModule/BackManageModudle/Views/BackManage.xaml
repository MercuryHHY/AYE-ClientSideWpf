<UserControl x:Class="BackManageModudle.Views.BackManage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             xmlns:ext="clr-namespace:BackManageModudle.Extensions"
             xmlns:local="clr-namespace:BackManageModudle.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <ContentControl Grid.Row="1" prism:RegionManager.RegionName="{x:Static ext:BackManager.BackManageViewRegionName}" />
    </Grid>
</UserControl>
